{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "defaultProfile": "{00000000-0000-0000-ba54-000000000001}",
    "profiles": {
        "defaults": {},
        "list": [
            {
                "guid": "{00000000-0000-0000-ba54-000000000001}",
                "closeOnExit" : true,
                "commandline" : "%UserProfile%/scoop/apps/git/current/bin/bash.exe",
                "fontFace" : "Fira Code Retina",
                "fontSize" : 12,
                "icon" : "D:/Server/giticon.png",
                "name" : "Git Bash",
                "padding" : "0, 0, 0, 0",
                "startingDirectory" : "D:/Developer/www"
            }
        ]
    },
    "schemes": [],
    "keybindings": [
        { "command": "newTab", "keys": [ "ctrl+t" ] },
        { "command": "closeTab", "keys": [ "ctrl+w" ] },
        { "command": "nextTab", "keys": [ "ctrl+shift+right" ] },
        { "command": "prevTab", "keys": [ "ctrl+shift+left" ] }
    ]
}